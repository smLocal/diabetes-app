  <div class="container">
    <div class="row row-date">
      <h3 class="dateHeadline"><%= todayIs %></h3>
    </div>

    <ul>
      <li class="row row-0" id="burgerLounge">
        <img src="../images/BurgerLoungeManagementLLC.png">
      </li>
      <li class="row row-1">
        <h1 class="col-md-12"><%= message[1]%></h1>
      </li>
      <li class="row row-2">
        <h1 class="col-md-12"><%= message[2]%></h1>
      </li>
      <li class="row row-3">
        <h1 class="col-md-12"><%= message[3]%></h1>
      </li>
    </ul>




  </div>

