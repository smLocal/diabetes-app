  <style type="text/css">
    body {
      width: 100%;
      height: 100%;
    }

    ul {
      list-style: none;
      width: 100%;
      height: 100%;
      padding-left: 0;
    }

    li:hover {
      border: 1px solid black;
    }

    #burgerLounge {
      width: 100%;
      height: 25%;
    }
  </style>

  <title>Meal Mates</title>
